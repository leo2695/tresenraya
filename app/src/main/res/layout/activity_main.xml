<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/pos1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:contentDescription="Casilla jugar gato"
        app:layout_constraintEnd_toStartOf="@+id/pos2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boton"
        app:srcCompat="@drawable/limpio" />


    <ImageButton
        android:id="@+id/pos3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="60dp"
        android:contentDescription="Casilla jugar gato"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boton"
        app:srcCompat="@drawable/limpio" />

    <ImageButton
        android:id="@+id/pos2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:contentDescription="Casilla jugar gato"
        app:layout_constraintEnd_toStartOf="@+id/pos3"
        app:layout_constraintTop_toBottomOf="@+id/boton"
        app:srcCompat="@drawable/limpio" />

    <Button
        android:id="@+id/boton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="32dp"
        android:text="@string/boton_ok"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/pos4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:contentDescription="Casilla jugar gato"
        app:layout_constraintEnd_toStartOf="@+id/pos5"
        app:layout_constraintHorizontal_bias="0.663"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pos1"
        app:srcCompat="@drawable/limpio" />

    <ImageButton
        android:id="@+id/pos5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="32dp"
        android:contentDescription="Casilla jugar gato"
        app:layout_constraintEnd_toStartOf="@+id/pos6"
        app:layout_constraintTop_toBottomOf="@+id/pos2"
        app:srcCompat="@drawable/limpio" />

    <ImageButton
        android:id="@+id/pos6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="60dp"
        android:contentDescription="Casilla jugar gato"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pos3"
        app:srcCompat="@drawable/limpio" />

    <ImageButton
        android:id="@+id/pos7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="24dp"
        android:contentDescription="Casilla jugar gato"
        app:layout_constraintEnd_toStartOf="@+id/pos8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pos4"
        app:srcCompat="@drawable/limpio" />

    <ImageButton
        android:id="@+id/pos8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="32dp"
        android:contentDescription="Casilla jugar gato"
        app:layout_constraintEnd_toStartOf="@+id/pos9"
        app:layout_constraintTop_toBottomOf="@+id/pos5"
        app:srcCompat="@drawable/limpio" />

    <ImageButton
        android:id="@+id/pos9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="60dp"
        android:contentDescription="Casilla jugar gato"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pos6"
        app:srcCompat="@drawable/limpio" />

</androidx.constraintlayout.widget.ConstraintLayout>